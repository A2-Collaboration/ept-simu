end sub
! ************* Dreiecke, Spitze nach oben **********
sub triangle_red siz 0 
begin origin
 begin path stroke fill red
  amove -siz/2 -.29*siz
  aline siz/2 -.29*siz
  aline 0 .58*siz
  closepath
 end path
end origin
end sub
