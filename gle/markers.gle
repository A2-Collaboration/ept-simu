  box siz siz justify cc
end sub
sub square_magenta siz 0
  box siz siz justify cc fill magenta
  box siz siz justify cc
end sub
sub square_yellow siz 0
  box siz siz justify cc fill yellow
  box siz siz justify cc
end sub
! ****** quadratische Rauten, Diamond **********
sub diamond_red siz 0
  begin rotate 45
  box siz siz justify cc fill red
  box siz siz justify cc
  end rotate
end sub
sub diamond_blue siz 0
  begin rotate 45
  box siz siz justify cc fill blue
  box siz siz justify cc
  end rotate
end sub
sub diamond_green siz 0
